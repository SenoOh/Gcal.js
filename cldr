#!/bin/bash
cmdpath=$(readlink $0 -f || echo $0)
cd $(dirname $cmdpath)

first=$1
second=$2
third=$3

if [ -z "$first" ] && [ -z "$second" ]; then
  node index.js
elif [ $first == "list" ]; then
  node index.js list
elif [ $first == "nm" ] && [ $second ]; then
  node index.js nm $second
elif [ $first == "nm" ] && [ -z "$second" ]; then
  node index.js nm
elif [ $first == "add" ]; then
  node index.js add
elif [ $first == "rm" ]; then
  node index.js rm
elif [ $first == "delete" ]; then
  node index.js delete 
elif [ $first == "md" ]; then
  node index.js md $second $third
elif [ $first == "show" ]; then
  node index.js show $second $third
else
  node index.js $first $second
fi
